<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-configuration PUBLIC "-//Hibernate/Hibernate Configuration DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-configuration-3.0.dtd">
<hibernate-configuration>
  <session-factory>
      <property name="hibernate.dialect">org.hibernate.dialect.MySQLDialect</property>
    <property name="hibernate.connection.driver_class">com.mysql.jdbc.Driver</property>
    <property name="hibernate.connection.url">jdbc:mysql://localhost:3306/test?zeroDateTimeBehavior=convertToNull</property>
    <property name="hibernate.connection.username">root</property>
    <property name="hibernate.hbm2ddl.auto">update</property>
    <mapping class="com.model.BIM1stSem"></mapping>
    <mapping class="com.model.BIM2ndSem"></mapping>
    <mapping class="com.model.BIM3rdSem"></mapping>
    <mapping class="com.model.BIM4thSem"></mapping>
    <mapping class="com.model.BIM5thSem"></mapping>
    <mapping class="com.model.BIM6thSem"></mapping>
    <mapping class="com.model.BIM7thSem"></mapping>
    <mapping class="com.model.BIM8thSem"></mapping>
 
  </session-factory>
</hibernate-configuration>
